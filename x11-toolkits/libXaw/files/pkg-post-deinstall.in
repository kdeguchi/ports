#!/bin/sh
/bin/rm -f ${PKG_ROOTDIR}${PKG_PREFIX}/lib/libXaw*
if [ -f ${PKG_ROOTDIR}${PKG_PREFIX}/lib/.libXaw3d/libXaw3d.so ]; then
	/usr/bin/install -l rs ${PKG_ROOTDIR}${PKG_PREFIX}/lib/.libXaw3d/* \
		${PKG_ROOTDIR}${PKG_PREFIX}/lib/
fi
if [ -f ${PKG_ROOTDIR}${PKG_PREFIX}/lib/.libxaw3dxft/libXaw3dxft.so ]; then
	/usr/bin/install -l rs ${PKG_ROOTDIR}${PKG_PREFIX}/lib/.libxaw3dxft/* \
		${PKG_ROOTDIR}${PKG_PREFIX}/lib/
fi
